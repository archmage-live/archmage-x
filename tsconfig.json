{
  "extends": "@archmagelive/plasmo/templates/tsconfig.base",
  "exclude": [
    "node_modules"
  ],
  "include": [
    ".plasmo/**/*",
    "./**/*.ts",
    "./**/*.tsx",
    "./types.d.ts"
  ],
  "compilerOptions": {
    "strict": true,
    "experimentalDecorators": true,
    "importsNotUsedAsValues": "remove",
    "paths": {
      "~*": [
        "./*"
      ]
    },
    "baseUrl": "."
  }
}
